PREFIX : <https://rdfox.com/examples/f1/>

# Count race wins

[?driver, :hasRaceWinCount, ?raceWinCount] :-
    AGGREGATE(
        [?result, :result_driver, ?driver],
        [?result, :result_positionOrder, 1] # Make sure the driver actually won 
        ON ?driver
        BIND COUNT(?result) AS ?raceWinCount
    ) .
